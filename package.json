{
  "name": "@gershy/compound-map",
  "version": "1.0.7",
  "description": "Like Map, but allows arbitrary sets of values for keys",
  "keywords": [
    "compound",
    "map"
  ],
  "author": "Gershom Maes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gershy/compound-map.git"
  },
  "bugs": {
    "url": "https://github.com/gershy/compound-map/issues"
  },
  "homepage": "https://github.com/gershy/compound-map#readme",
  "license": "ISC",
  "exports": {
    ".": {
      "import": "./cmp/mjs/main.js",
      "require": "./cmp/cjs/main.js"
    }
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "cpx": "^1.5.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  },
  "files": [
    "cmp"
  ],
  "types": "./cmp/mjs/main.d.ts",
  "scripts": {
    "build.cjs": "tsc -p ts/tsconfig.cjs.json",
    "build.mjs": "tsc -p ts/tsconfig.mjs.json",
    "build": "rimraf cmp && npm run build.cjs && npm run build.mjs",
    "npm.pub": "npm run build && npm publish --access public",
    "git.pub": "git add --all && git commit -m \"automated\" && git push"
  }
}
